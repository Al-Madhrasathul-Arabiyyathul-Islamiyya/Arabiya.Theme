<Application
  x:Class="Arabiyya.Theme.Demo.App"
  xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:local="using:Arabiyya.Theme.Demo"
  RequestedThemeVariant="Default">

  <Application.Styles>
    <FluentTheme />
    <StyleInclude Source="avares://Arabiyya.Theme/Theme.axaml" />
  </Application.Styles>

  <Application.Resources>
    <x:Boolean x:Key="IsDarkTheme">false</x:Boolean>
  </Application.Resources>
</Application>