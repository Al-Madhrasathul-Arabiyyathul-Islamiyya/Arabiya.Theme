<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <ResourceDictionary.ThemeDictionaries>
    <ResourceDictionary x:Key="Light">
      <!--  Light Theme Colors - with both prefixed and regular keys  -->
      <Color x:Key="PrimaryColor">#0072B5</Color>
      <Color x:Key="LightPrimaryColor">#0072B5</Color>

      <Color x:Key="PrimaryLightColor">#3A95D0</Color>
      <Color x:Key="LightPrimaryLightColor">#3A95D0</Color>

      <Color x:Key="PrimaryDarkColor">#005A91</Color>
      <Color x:Key="LightPrimaryDarkColor">#005A91</Color>

      <Color x:Key="SecondaryColor">#7D3C98</Color>
      <Color x:Key="LightSecondaryColor">#7D3C98</Color>

      <Color x:Key="SecondaryLightColor">#9B59B6</Color>
      <Color x:Key="LightSecondaryLightColor">#9B59B6</Color>

      <Color x:Key="SecondaryDarkColor">#5B2C6F</Color>
      <Color x:Key="LightSecondaryDarkColor">#5B2C6F</Color>

      <Color x:Key="AccentColor">#FF9800</Color>
      <Color x:Key="LightAccentColor">#FF9800</Color>

      <Color x:Key="AccentLightColor">#FFB74D</Color>
      <Color x:Key="LightAccentLightColor">#FFB74D</Color>

      <Color x:Key="AccentDarkColor">#F57C00</Color>
      <Color x:Key="LightAccentDarkColor">#F57C00</Color>

      <!--  Status Colors  -->
      <Color x:Key="SuccessColor">#2E7D32</Color>
      <Color x:Key="LightSuccessColor">#2E7D32</Color>

      <Color x:Key="SuccessLightColor">#4CAF50</Color>
      <Color x:Key="LightSuccessLightColor">#4CAF50</Color>

      <Color x:Key="SuccessDarkColor">#1B5E20</Color>
      <Color x:Key="LightSuccessDarkColor">#1B5E20</Color>

      <Color x:Key="ErrorColor">#C62828</Color>
      <Color x:Key="LightErrorColor">#C62828</Color>

      <Color x:Key="ErrorLightColor">#EF5350</Color>
      <Color x:Key="LightErrorLightColor">#EF5350</Color>

      <Color x:Key="ErrorDarkColor">#B71C1C</Color>
      <Color x:Key="LightErrorDarkColor">#B71C1C</Color>

      <Color x:Key="WarningColor">#F57C00</Color>
      <Color x:Key="LightWarningColor">#F57C00</Color>

      <Color x:Key="WarningLightColor">#FFB74D</Color>
      <Color x:Key="LightWarningLightColor">#FFB74D</Color>

      <Color x:Key="WarningDarkColor">#E65100</Color>
      <Color x:Key="LightWarningDarkColor">#E65100</Color>

      <Color x:Key="InfoColor">#1565C0</Color>
      <Color x:Key="LightInfoColor">#1565C0</Color>

      <Color x:Key="InfoLightColor">#42A5F5</Color>
      <Color x:Key="LightInfoLightColor">#42A5F5</Color>

      <Color x:Key="InfoDarkColor">#0D47A1</Color>
      <Color x:Key="LightInfoDarkColor">#0D47A1</Color>

      <!--  UI Colors  -->
      <Color x:Key="BackgroundColor">#F8FDFA</Color>
      <Color x:Key="LightBackgroundColor">#F8FDFA</Color>

      <Color x:Key="SurfaceColor">#FFFFFF</Color>
      <Color x:Key="LightSurfaceColor">#FFFFFF</Color>

      <Color x:Key="SurfaceElevated1Color">#FFFFFF</Color>
      <Color x:Key="LightSurfaceElevated1Color">#FFFFFF</Color>

      <Color x:Key="SurfaceElevated2Color">#FFFFFF</Color>
      <Color x:Key="LightSurfaceElevated2Color">#FFFFFF</Color>

      <!--  Text Colors  -->
      <Color x:Key="TextPrimaryColor">#212121</Color>
      <Color x:Key="LightTextPrimaryColor">#212121</Color>

      <Color x:Key="TextSecondaryColor">#424242</Color>
      <Color x:Key="LightTextSecondaryColor">#424242</Color>

      <Color x:Key="TextDisabledColor">#9E9E9E</Color>
      <Color x:Key="LightTextDisabledColor">#9E9E9E</Color>

      <Color x:Key="TextOnPrimaryColor">#FFFFFF</Color>
      <Color x:Key="LightTextOnPrimaryColor">#FFFFFF</Color>

      <Color x:Key="TextOnSecondaryColor">#FFFFFF</Color>
      <Color x:Key="LightTextOnSecondaryColor">#FFFFFF</Color>

      <Color x:Key="TextOnAccentColor">#000000</Color>
      <Color x:Key="LightTextOnAccentColor">#000000</Color>

      <Color x:Key="TextColorDark">#020617</Color>
      <Color x:Key="LightTextColorDark">#020617</Color>

      <Color x:Key="TextColorLight">#f8fafc</Color>
      <Color x:Key="LightTextColorLight">#f8fafc</Color>

      <Color x:Key="TextColorMuted">#64748b</Color>
      <Color x:Key="LightTextColorMuted">#64748b</Color>

      <Color x:Key="TextColorDisabled">#cbd5e1</Color>
      <Color x:Key="LightTextColorDisabled">#cbd5e1</Color>

      <!--  Border Colors  -->
      <Color x:Key="DividerColor">#E2E8F0</Color>
      <Color x:Key="LightDividerColor">#E2E8F0</Color>

      <Color x:Key="OutlineColor">#F1F5F9</Color>
      <Color x:Key="LightOutlineColor">#F1F5F9</Color>

      <!--  Background Gradients  -->
      <LinearGradientBrush x:Key="ThemeBackgroundGradient" StartPoint="0%,0%" EndPoint="100%,100%">
        <GradientStop Offset="0" Color="#11998e" />
        <GradientStop Offset="1" Color="#38ef7d" />
      </LinearGradientBrush>

      <LinearGradientBrush x:Key="LightThemeGradient" StartPoint="0%,0%" EndPoint="100%,100%">
        <GradientStop Offset="0" Color="#11998e" />
        <GradientStop Offset="1" Color="#38ef7d" />
      </LinearGradientBrush>

      <LinearGradientBrush x:Key="GlassGradient" StartPoint="0%,0%" EndPoint="0%,100%">
        <GradientStop Offset="0" Color="#19FFFFFF" />
        <GradientStop Offset="1" Color="#0CFFFFFF" />
      </LinearGradientBrush>

      <LinearGradientBrush x:Key="LightGlassGradient" StartPoint="0%,0%" EndPoint="0%,100%">
        <GradientStop Offset="0" Color="#19FFFFFF" />
        <GradientStop Offset="1" Color="#0CFFFFFF" />
      </LinearGradientBrush>

      <!--  Brand Color Gradients  -->
      <LinearGradientBrush x:Key="PrimaryGradient" StartPoint="0%,0%" EndPoint="100%,0%">
        <GradientStop Offset="0" Color="{StaticResource PrimaryColor}" />
        <GradientStop Offset="1" Color="{StaticResource PrimaryLightColor}" />
      </LinearGradientBrush>

      <LinearGradientBrush x:Key="PrimaryVerticalGradient" StartPoint="0%,0%" EndPoint="0%,100%">
        <GradientStop Offset="0" Color="{StaticResource PrimaryColor}" />
        <GradientStop Offset="1" Color="{StaticResource PrimaryLightColor}" />
      </LinearGradientBrush>

      <LinearGradientBrush x:Key="SecondaryGradient" StartPoint="0%,0%" EndPoint="100%,0%">
        <GradientStop Offset="0" Color="{StaticResource SecondaryColor}" />
        <GradientStop Offset="1" Color="{StaticResource SecondaryLightColor}" />
      </LinearGradientBrush>

      <LinearGradientBrush x:Key="SecondaryVerticalGradient" StartPoint="0%,0%" EndPoint="0%,100%">
        <GradientStop Offset="0" Color="{StaticResource SecondaryColor}" />
        <GradientStop Offset="1" Color="{StaticResource SecondaryLightColor}" />
      </LinearGradientBrush>

      <LinearGradientBrush x:Key="AccentGradient" StartPoint="0%,0%" EndPoint="100%,0%">
        <GradientStop Offset="0" Color="{StaticResource AccentColor}" />
        <GradientStop Offset="1" Color="{StaticResource AccentLightColor}" />
      </LinearGradientBrush>

      <LinearGradientBrush x:Key="AccentVerticalGradient" StartPoint="0%,0%" EndPoint="0%,100%">
        <GradientStop Offset="0" Color="{StaticResource AccentColor}" />
        <GradientStop Offset="1" Color="{StaticResource AccentLightColor}" />
      </LinearGradientBrush>

      <!--  Status Gradients  -->
      <LinearGradientBrush x:Key="SuccessGradient" StartPoint="0%,0%" EndPoint="100%,0%">
        <GradientStop Offset="0" Color="{StaticResource SuccessDarkColor}" />
        <GradientStop Offset="1" Color="{StaticResource SuccessColor}" />
      </LinearGradientBrush>

      <LinearGradientBrush x:Key="ErrorGradient" StartPoint="0%,0%" EndPoint="100%,0%">
        <GradientStop Offset="0" Color="{StaticResource ErrorDarkColor}" />
        <GradientStop Offset="1" Color="{StaticResource ErrorColor}" />
      </LinearGradientBrush>

      <LinearGradientBrush x:Key="WarningGradient" StartPoint="0%,0%" EndPoint="100%,0%">
        <GradientStop Offset="0" Color="{StaticResource WarningDarkColor}" />
        <GradientStop Offset="1" Color="{StaticResource WarningColor}" />
      </LinearGradientBrush>

      <LinearGradientBrush x:Key="InfoGradient" StartPoint="0%,0%" EndPoint="100%,0%">
        <GradientStop Offset="0" Color="{StaticResource InfoDarkColor}" />
        <GradientStop Offset="1" Color="{StaticResource InfoColor}" />
      </LinearGradientBrush>

      <!--  Special Effect Gradients  -->
      <LinearGradientBrush x:Key="CalmWaterGradient" StartPoint="0%,0%" EndPoint="100%,0%">
        <GradientStop Offset="0" Color="#3494E6" />
        <GradientStop Offset="1" Color="#EC6EAD" />
      </LinearGradientBrush>

      <LinearGradientBrush x:Key="SunriseGradient" StartPoint="0%,0%" EndPoint="100%,0%">
        <GradientStop Offset="0" Color="#FF512F" />
        <GradientStop Offset="1" Color="#F09819" />
      </LinearGradientBrush>

      <LinearGradientBrush x:Key="PurpleDreamGradient" StartPoint="0%,0%" EndPoint="100%,0%">
        <GradientStop Offset="0" Color="#8E2DE2" />
        <GradientStop Offset="1" Color="#4A00E0" />
      </LinearGradientBrush>

      <LinearGradientBrush x:Key="OceanViewGradient" StartPoint="0%,0%" EndPoint="100%,0%">
        <GradientStop Offset="0" Color="#4AC29A" />
        <GradientStop Offset="1" Color="#BDFFF3" />
      </LinearGradientBrush>

      <LinearGradientBrush x:Key="SunsetGradient" StartPoint="0%,0%" EndPoint="100%,0%">
        <GradientStop Offset="0" Color="#FF416C" />
        <GradientStop Offset="1" Color="#FF4B2B" />
      </LinearGradientBrush>

      <LinearGradientBrush x:Key="MainBackgroundGradient" StartPoint="0%,0%" EndPoint="100%,100%">
        <GradientStop Offset="0" Color="#11998e" />
        <GradientStop Offset="1" Color="#38ef7d" />
      </LinearGradientBrush>

      <!--  Glass Button Values  -->
      <Color x:Key="GlassButtonBorderColor">#40FFFFFF</Color>
      <Color x:Key="GlassButtonHoverBorderColor">#60FFFFFF</Color>
      <Color x:Key="GlassButtonPressedBorderColor">#50FFFFFF</Color>
      <Color x:Key="GlassButtonPressedBackgroundColor">#25FFFFFF</Color>
      <Color x:Key="GlassButtonDisabledBorderColor">#20FFFFFF</Color>
      <Color x:Key="GlassButtonDisabledBackgroundColor">#05FFFFFF</Color>

      <!--  Brushes  -->
      <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}" />
      <SolidColorBrush x:Key="PrimaryLightBrush" Color="{StaticResource PrimaryLightColor}" />
      <SolidColorBrush x:Key="PrimaryDarkBrush" Color="{StaticResource PrimaryDarkColor}" />

      <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource SecondaryColor}" />
      <SolidColorBrush x:Key="SecondaryLightBrush" Color="{StaticResource SecondaryLightColor}" />
      <SolidColorBrush x:Key="SecondaryDarkBrush" Color="{StaticResource SecondaryDarkColor}" />

      <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}" />
      <SolidColorBrush x:Key="AccentLightBrush" Color="{StaticResource AccentLightColor}" />
      <SolidColorBrush x:Key="AccentDarkBrush" Color="{StaticResource AccentDarkColor}" />

      <SolidColorBrush x:Key="SuccessBrush" Color="{StaticResource SuccessColor}" />
      <SolidColorBrush x:Key="SuccessLightBrush" Color="{StaticResource SuccessLightColor}" />
      <SolidColorBrush x:Key="SuccessDarkBrush" Color="{StaticResource SuccessDarkColor}" />

      <SolidColorBrush x:Key="ErrorBrush" Color="{StaticResource ErrorColor}" />
      <SolidColorBrush x:Key="ErrorLightBrush" Color="{StaticResource ErrorLightColor}" />
      <SolidColorBrush x:Key="ErrorDarkBrush" Color="{StaticResource ErrorDarkColor}" />

      <SolidColorBrush x:Key="WarningBrush" Color="{StaticResource WarningColor}" />
      <SolidColorBrush x:Key="WarningLightBrush" Color="{StaticResource WarningLightColor}" />
      <SolidColorBrush x:Key="WarningDarkBrush" Color="{StaticResource WarningDarkColor}" />

      <SolidColorBrush x:Key="InfoBrush" Color="{StaticResource InfoColor}" />
      <SolidColorBrush x:Key="InfoLightBrush" Color="{StaticResource InfoLightColor}" />
      <SolidColorBrush x:Key="InfoDarkBrush" Color="{StaticResource InfoDarkColor}" />

      <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}" />
      <SolidColorBrush x:Key="SurfaceBrush" Color="{StaticResource SurfaceColor}" />
      <SolidColorBrush x:Key="SurfaceElevated1Brush" Color="{StaticResource SurfaceElevated1Color}" />
      <SolidColorBrush x:Key="SurfaceElevated2Brush" Color="{StaticResource SurfaceElevated2Color}" />

      <SolidColorBrush x:Key="TextPrimaryBrush" Color="{StaticResource TextPrimaryColor}" />
      <SolidColorBrush x:Key="TextSecondaryBrush" Color="{StaticResource TextSecondaryColor}" />
      <SolidColorBrush x:Key="TextDisabledBrush" Color="{StaticResource TextDisabledColor}" />
      <SolidColorBrush x:Key="TextOnPrimaryBrush" Color="{StaticResource TextOnPrimaryColor}" />
      <SolidColorBrush x:Key="TextOnSecondaryBrush" Color="{StaticResource TextOnSecondaryColor}" />
      <SolidColorBrush x:Key="TextOnAccentBrush" Color="{StaticResource TextOnAccentColor}" />

      <SolidColorBrush x:Key="TextColorDarkBrush" Color="{StaticResource TextColorDark}" />
      <SolidColorBrush x:Key="TextColorLightBrush" Color="{StaticResource TextColorLight}" />
      <SolidColorBrush x:Key="TextColorMutedBrush" Color="{StaticResource TextColorMuted}" />
      <SolidColorBrush x:Key="TextColorDisabledBrush" Color="{StaticResource TextColorDisabled}" />

      <SolidColorBrush x:Key="DividerBrush" Color="{StaticResource DividerColor}" />
      <SolidColorBrush x:Key="OutlineBrush" Color="{StaticResource OutlineColor}" />

      <SolidColorBrush x:Key="GlassButtonBorderBrush" Color="{StaticResource GlassButtonBorderColor}" />
      <SolidColorBrush x:Key="GlassButtonHoverBorderBrush" Color="{StaticResource GlassButtonHoverBorderColor}" />
      <SolidColorBrush x:Key="GlassButtonPressedBorderBrush" Color="{StaticResource GlassButtonPressedBorderColor}" />
      <SolidColorBrush x:Key="GlassButtonPressedBackgroundBrush" Color="{StaticResource GlassButtonPressedBackgroundColor}" />
      <SolidColorBrush x:Key="GlassButtonDisabledBorderBrush" Color="{StaticResource GlassButtonDisabledBorderColor}" />
      <SolidColorBrush x:Key="GlassButtonDisabledBackgroundBrush" Color="{StaticResource GlassButtonDisabledBackgroundColor}" />

      <x:String x:Key="PanelThemeClass">GlassPanel</x:String>
    </ResourceDictionary>
  </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>